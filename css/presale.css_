html {
    height: 100%;
    width: 100%;
    transition: opacity .3s;
    -webkit-transition: opacity .3s;
    -moz-transition: opacity .3s;
    -o-transition: opacity .3s;
    -ms-transition: opacity .3s
}
body {
    height: 100%;
    margin: 0 auto;
    background: #fff;
    color: #0c041e;
    font-family: 'Titillium Web', sans-serif;
    font-weight: 100
}
p {
    font-family: book;
    font-size: 14px;
