.txt-highlight {
    opacity: 1;
    -webkit-transform: translateZ(0);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-image: -webkit-linear-gradient(252deg, #2cbfdf, #2c9cdf);
    background-image: -o-linear-gradient(252deg, #2cbfdf, #2c9cdf);
    background-image: -moz-linear-gradient(252deg, #2cbfdf, #2c9cdf);
    background-image: linear-gradient(252deg, #2cbfdf, #2c9cdf)
}
.txt-highlight.warning {
    background-image: -webkit-linear-gradient(79deg, #E60E2B 0%, #FF6A0D 100%);
    background-image: -o-linear-gradient(79deg, #E60E2B 0%, #FF6A0D 100%);
    background-image: -moz-linear-gradient(79deg, #E60E2B 0%, #FF6A0D 100%);
    background-image: linear-gradient(11deg, #E60E2B 0%, #FF6A0D 100%);
}
